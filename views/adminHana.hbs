                <th>Odo Meter</th>
                <th>Equipment</th>
                <th>Price</th>
                <th>Transmission</th>
                <th>Images</th>
                <th>Changing</th>
            </tr>
            {{#each data}}
            <tr>

                <td>{{this.lotNumber}}</td>
                <td>{{this.carModel}}</td>
                <td>{{this.chassisNumber}}</td>
                <td>{{this.odoMeter}}</td>
                <td>{{this.equipment}}</td>
                <td>{{this.price}}</td>
                <td>{{this.transmission}}</td>
                <td><img src="/{{this.imageUpload}}" alt="{{this.imageUpload}}" width="100px" height="100px"></td>
                <td>
                    <button id="deletBTN"><a href="/adminPanel/adminHana/{{this._id}}">Delete</a></button>
                    <button id="updateBTN"><a href="/adminPanel/{{this._id}}">Update</a></button>
                </td>
            </tr>
            {{/each}}
        </table>
 	
